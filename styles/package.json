  "description": "knutsen.co source.",
  "main": "pages/index.js",
  "now": {
    "name": "knutsen",
    "alias": "knutsen.co"
  },
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start -p 8000"
  },
  "keywords": [
    "personal",
    "website",
    "knutsen"
  ],
  "author": {
    "name": "Fredrik",
    "url": "https://github.com/Demonstrandum/"
  },
  "license": "GPL-2.0",
  "dependencies": {
    "@zeit/next-css": "^0.2.0",
    "next": "^9.4.4",
    "react": "^16.4.2",
    "react-dom": "^16.4.2"
  },
  "devDependencies": {
    "eslint-config-xo-react": "^0.17.0",
    "eslint-plugin-react": "^7.11.1",
    "xo": "^0.22.0"
  },
  "xo": {
    "extends": "xo-react",
    "space": true,
    "rules": {
      "camelcase": "off",
      "unicorn/filename-case": "off",
      "new-cap": "off",
      "no-mixed-operators": "off",
      "no-undef": "off",
      "import/extensions": "off",
      "max-params": "off",
      "no-warning-comments": "off",
      "no-multi-spaces": "off",
      "func-name-matching": "off",
      "func-names": "off",
      "no-use-extend-native/no-use-extend-native": "off",
      "no-extend-native": "off",
      "no-unused-expressions": "off",
      "unicorn/escape-case": "off",
      "react/react-in-jsx-scope": "off",
      "import/no-unassigned-import": "off",
      "react/prop-types": "off",
      "no-unused-vars": "off",
      "react/jsx-no-undef": "off"
    }
  }
}
